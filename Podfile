  pod 'Masonry'
  pod 'WSProgressHUD'
  pod 'YYCache'
  target 'VBKitExampleTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'VBKitExampleUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
